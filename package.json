  "scripts": {
    "start": "yarn start:shell",
    "start:shell": "yarn nx serve shell",
    "start:shell:all": "yarn nx serve shell --devRemotes=staking,governance",
    "start:staking": "yarn nx serve staking",
    "start:governance": "yarn nx serve governance",
    "start:vesting": "yarn nx serve vesting",
    "start:faucet": "yarn nx serve faucet",
    "start:haqq-website": "yarn nx serve haqq-website",
    "start:islamic-website": "yarn nx serve islamic-website",
    "format": "yarn prettier --write '**/*.{ts,js,tsx,jsx,cjs,mjs,css,json,md,mdx,html,yaml,yml}'",
    "prepare": "is-ci || husky install",
    "check-ignore-build": "yarn node ./tools/scripts/ignore-build.mjs",
    "patch:viem": "yarn node ./tools/scripts/viem-module-federation-patch.mjs",
    "postinstall": "yarn prepare && yarn patch:viem"
  },
